{
    "name": "yamanhacioglu/omnipay-billwerk",
    "type": "library",
    "description": "Billwerk (Reepay) gateway for Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "billwerk",
        "reepay",
        "purchase",
        "subscription"
    ],
    "homepage": "https://github.com/thephpleague/omnipay-billwerk",
    "license": "MIT",
    "authors": [
        {
            "name": "Yaman HACIOÄžLU - Northlab Software",
            "homepage": "https://github.com/yamanhacioglu/omnipay-billwerk/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\Billwerk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\Billwerk\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "omnipay/common": "^3.0",
        "billwerk/php-sdk": "^1.0"
    },
    "require-dev": {
        "omnipay/tests": "^4.0",
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Omnipay\\Billwerk\\BillwerkServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
